{
  "name": "@devserver/cli",
  "description": "Minimalist development server",
  "type": "module",
  "version": "0.6.0",
  "scripts": {
    "example:dev": "node src/cli.js dev example --cdn --ssl",
    "example:dev:cjs": "node ./cli.cjs dev example --cdn --ssl",
    "example:build": "node src/cli.js build example/*.html public",
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "UpperCod",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/UpperCod/devserver.git"
  },
  "license": "ISC",
  "dependencies": {
    "@devserver/replace-import": "^0.3.0",
    "sade": "^1.7.4"
  },
  "devDependencies": {
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@devserver/build": "^0.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@uppercod/exp-route": "^1.2.1",
    "atomico": "^1.9.1",
    "ava": "^3.14.0",
    "https-localhost": "^4.6.4",
    "mime": "^2.4.7",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "bin": {
    "devserver": "./cli.cjs"
  }
}
